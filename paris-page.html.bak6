    <style>
        :root {
            --primary-gradient: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            --secondary-gradient: linear-gradient(135deg, #f093fb 0%, #f5576c 100%);
            --accent-gradient: linear-gradient(135deg, #4facfe 0%, #00f2fe 100%);
            --paris-gradient: linear-gradient(135deg, #ff9a9e 0%, #fecfef 50%, #fecfef 100%);
            --text-primary: #2c3e50;
            --text-secondary: #6c757d;
            --text-light: #ffffff;
            --card-bg: rgba(255, 255, 255, 0.95);
            --shadow-light: 0 4px 6px rgba(0, 0, 0, 0.07);
            --shadow-medium: 0 10px 25px rgba(0, 0, 0, 0.1);
            --shadow-heavy: 0 20px 40px rgba(0, 0, 0, 0.15);
            --border-radius: 20px;
            --transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: 'Inter', system-ui, -apple-system, sans-serif;
            line-height: 1.6;
            color: var(--text-primary);
            background: linear-gradient(135deg, #f5f7fa 0%, #c3cfe2 100%);
            min-height: 100vh;
            opacity: 0;
            transition: opacity 0.5s ease-in-out;
        }

        /* Navigation Bar */
        .nav-bar {
            position: fixed;
            top: 0;
            left: 0;
            right: 0;
            background: rgba(255, 255, 255, 0.95);
            backdrop-filter: blur(20px);
            padding: 15px 0;
            z-index: 1000;
            box-shadow: var(--shadow-light);
        }

        .nav-container {
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 20px;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .home-btn {
            display: inline-flex;
            align-items: center;
            gap: 8px;
            color: var(--text-light);
            background: var(--primary-gradient);
            text-decoration: none;
            padding: 10px 20px;
            border-radius: 25px;
            font-weight: 500;
            font-size: 0.9rem;
            transition: var(--transition);
            box-shadow: var(--shadow-light);
        }

        .home-btn:hover {
            transform: translateY(-2px);
            box-shadow: var(--shadow-medium);
        }

        .location-tag {
            display: inline-flex;
            align-items: center;
            gap: 6px;
            background: var(--paris-gradient);
            color: var(--text-primary);
            padding: 8px 16px;
            border-radius: 20px;
            font-size: 0.85rem;
            font-weight: 500;
        }

        /* Hero Section */
        .hero {
            height: 100vh;
            position: relative;
            display: flex;
            align-items: center;
            justify-content: center;
            overflow: hidden;
            background: #000;
        }

        /* Paris Collage Background */
        .hero-collage {
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            display: flex;
        }

        .hero-left {
            flex: 1;
            position: relative;
            overflow: hidden;
            transform: skewX(-8deg);
            margin-left: -5%;
        }

        .hero-right {
            flex: 1;
            position: relative;
            overflow: hidden;
            transform: skewX(-8deg);
            margin-right: -5%;
        }

        .collage-image {
            position: absolute;
            width: 100%;
            height: 100%;
            object-fit: cover;
            transform: skewX(8deg) scale(1.3);
            filter: brightness(0.7) saturate(1.2) contrast(1.1);
        }

        .hero-left .collage-image,
        .hero-right .collage-image {
            animation: kenBurns 20s ease-in-out infinite;
        }

        .hero-left .collage-image:nth-child(1) { animation-delay: 0s; }
        .hero-left .collage-image:nth-child(2) { animation-delay: 4s; }
        .hero-left .collage-image:nth-child(3) { animation-delay: 8s; }
        .hero-left .collage-image:nth-child(4) { animation-delay: 12s; }
        .hero-left .collage-image:nth-child(5) { animation-delay: 16s; }

        .hero-right .collage-image:nth-child(1) { animation-delay: 2s; }
        .hero-right .collage-image:nth-child(2) { animation-delay: 6s; }
        .hero-right .collage-image:nth-child(3) { animation-delay: 10s; }
        .hero-right .collage-image:nth-child(4) { animation-delay: 14s; }
        .hero-right .collage-image:nth-child(5) { animation-delay: 18s; }

        @keyframes kenBurns {
            0% { opacity: 0; transform: skewX(8deg) scale(1.3); }
            5% { opacity: 1; }
            20% { opacity: 1; transform: skewX(8deg) scale(1.4); }
            25% { opacity: 0; }
            100% { opacity: 0; transform: skewX(8deg) scale(1.3); }
        }

        .hero::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: linear-gradient(135deg, rgba(255, 154, 158, 0.3) 0%, rgba(254, 207, 239, 0.2) 50%, rgba(250, 208, 196, 0.3) 100%);
            z-index: 1;
            pointer-events: none;
        }

        @keyframes subtleFloat {
            0%, 100% {
                transform: translateY(0);
            }
            50% {
                transform: translateY(-10px);
            }
        }

        .hero::after {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: radial-gradient(circle at center, transparent 40%, rgba(0,0,0,0.2) 90%);
            z-index: 2;
        }

        .hero-content {
            position: relative;
            z-index: 2;
            text-align: center;
            color: var(--text-light);
            max-width: 900px;
            padding: 3rem;
            background: rgba(0, 0, 0, 0.4);
            backdrop-filter: blur(20px);
            border-radius: 30px;
            border: 2px solid rgba(255, 255, 255, 0.2);
            box-shadow: 0 20px 60px rgba(0, 0, 0, 0.3);
        }

        .hero h1 {
            font-family: 'Playfair Display', serif;
            font-size: clamp(3rem, 8vw, 6rem);
            font-weight: 700;
            margin-bottom: 1rem;
            text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.3);
            color: white;
            animation: slideInUp 1s ease-out 0.5s both;
        }

        .hero-subtitle {
            font-size: clamp(1.2rem, 3vw, 2rem);
            font-weight: 300;
            margin-bottom: 2rem;
            opacity: 0.9;
            color: white;
            animation: slideInUp 1s ease-out 0.7s both;
        }

        .hero-description {
            font-size: clamp(1rem, 2vw, 1.2rem);
            line-height: 1.8;
            opacity: 0.8;
            color: white;
            animation: slideInUp 1s ease-out 0.9s both;
        }

        @keyframes slideInUp {
            from {
                opacity: 0;
                transform: translateY(30px);
            }
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        /* Main Content */
        main {
            max-width: 1200px;
            margin: 0 auto;
            padding: 4rem 2rem;
            position: relative;
        }

        /* Section Styling */
        .content-section {
            margin-bottom: 80px;
        }

        .section-title {
            font-family: 'Playfair Display', serif;
            font-size: clamp(2rem, 4vw, 2.8rem);
            font-weight: 600;
            background: var(--paris-gradient);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
            margin-bottom: 30px;
            text-align: center;
        }

        .description-card {
            background: var(--card-bg);
            border-radius: var(--border-radius);
            padding: 40px;
            margin-bottom: 50px;
            box-shadow: var(--shadow-medium);
            backdrop-filter: blur(20px);
            border: 1px solid rgba(255, 255, 255, 0.2);
            position: relative;
            overflow: hidden;
        }

        .description-card::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 4px;
            background: var(--paris-gradient);
        }

        .description-card p {
            font-size: 1.1rem;
            line-height: 1.8;
            color: var(--text-secondary);
            margin-bottom: 0;
        }

        /* Image Gallery */
        .image-gallery {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
            gap: 30px;
            margin-top: 40px;
            align-items: start;
        }

        .gallery-item {
            background: var(--card-bg);
            border-radius: var(--border-radius);
            overflow: hidden;
            box-shadow: var(--shadow-light);
            transition: var(--transition);
            border: 1px solid rgba(255, 255, 255, 0.2);
            backdrop-filter: blur(20px);
        }

        .gallery-item:hover {
            transform: translateY(-10px);
            box-shadow: var(--shadow-heavy);
        }

        .gallery-image-container {
            position: relative;
            overflow: hidden;
            width: 100%;
        }

        .gallery-image {
            width: 100%;
            height: auto;
            max-height: 400px;
            object-fit: contain;
            object-position: center;
            display: block;
            transition: var(--transition);
            border-radius: var(--border-radius) var(--border-radius) 0 0;
        }

        .gallery-item:hover .gallery-image {
            transform: scale(1.05);
        }

        .gallery-caption {
            padding: 25px;
        }

        .gallery-caption h3 {
            font-family: 'Playfair Display', serif;
            font-size: 1.5rem;
            font-weight: 600;
            margin-bottom: 10px;
            color: var(--text-primary);
        }

        .gallery-caption p {
            color: var(--text-secondary);
            font-size: 0.95rem;
            line-height: 1.6;
        }

        /* Carousel Styles */
        .carousel-container {
            position: relative;
            width: 100%;
            height: 100%;
            overflow: hidden;
            border-radius: var(--border-radius) var(--border-radius) 0 0;
        }

        .carousel-wrapper {
            display: flex;
            transition: transform 0.3s ease-in-out;
            height: 100%;
        }

        .carousel-slide {
            min-width: 100%;
            height: 100%;
            position: relative;
        }

        .carousel-slide.active {
            display: block;
        }

        .carousel-slide img {
            width: 100%;
            height: 100%;
            object-fit: contain;
            display: block;
        }

        .carousel-nav {
            position: absolute;
            top: 50%;
            transform: translateY(-50%);
            background: rgba(255, 255, 255, 0.9);
            border: none;
            width: 40px;
            height: 40px;
            border-radius: 50%;
            cursor: pointer;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 1.2rem;
            color: var(--text-primary);
            transition: var(--transition);
            z-index: 10;
            backdrop-filter: blur(10px);
        }

        .carousel-nav:hover {
            background: rgba(255, 255, 255, 1);
            transform: translateY(-50%) scale(1.1);
        }

        .carousel-prev {
            left: 15px;
        }

        .carousel-next {
            right: 15px;
        }

        .carousel-indicators {
            position: absolute;
            bottom: 15px;
            left: 50%;
            transform: translateX(-50%);
            display: flex;
            gap: 8px;
            z-index: 10;
        }

        .carousel-indicator {
            width: 8px;
            height: 8px;
            border-radius: 50%;
            background: rgba(255, 255, 255, 0.5);
            cursor: pointer;
            transition: var(--transition);
        }

        .carousel-indicator.active {
            background: rgba(255, 255, 255, 1);
            transform: scale(1.2);
        }

        .carousel-counter {
            position: absolute;
            top: 15px;
            right: 15px;
            background: rgba(0, 0, 0, 0.7);
            color: white;
            padding: 5px 10px;
            border-radius: 15px;
            font-size: 0.85rem;
            font-weight: 500;
            z-index: 10;
        }

        /* Stats Grid */
        .stats-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            gap: 25px;
            margin: 50px 0;
        }

        .stat-card {
            background: var(--card-bg);
            border-radius: var(--border-radius);
            padding: 30px 20px;
            text-align: center;
            box-shadow: var(--shadow-light);
            backdrop-filter: blur(20px);
            border: 1px solid rgba(255, 255, 255, 0.2);
            transition: var(--transition);
        }

        .stat-card:hover {
            transform: translateY(-5px);
            box-shadow: var(--shadow-medium);
        }

        .stat-number {
            font-size: 2.5rem;
            font-weight: 700;
            background: var(--paris-gradient);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
            margin-bottom: 10px;
        }

        .stat-label {
            color: var(--text-secondary);
            font-size: 0.9rem;
            font-weight: 500;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        /* Quote Section */
        .quote-section {
            text-align: center;
            padding: 60px 40px;
            background: var(--card-bg);
            border-radius: var(--border-radius);
            box-shadow: var(--shadow-medium);
            backdrop-filter: blur(20px);
            border: 1px solid rgba(255, 255, 255, 0.2);
            position: relative;
            overflow: hidden;
        }

        .quote-section::before {
            content: '';
            position: absolute;
            top: -50%;
            left: -50%;
            width: 200%;
            height: 200%;
            background: var(--paris-gradient);
            opacity: 0.03;
            transform: rotate(45deg);
        }

        .quote-text {
            font-family: 'Playfair Display', serif;
            font-size: clamp(1.5rem, 4vw, 2.5rem);
            font-weight: 600;
            font-style: italic;
            background: var(--paris-gradient);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
            margin-bottom: 20px;
            position: relative;
        }

        .quote-author {
            font-size: 1.1rem;
            color: var(--text-secondary);
            font-weight: 500;
        }

        /* Travel Tips Grid */
        .travel-tips-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 25px;
            margin-top: 40px;
        }

        .tip-card {
            background: var(--card-bg);
            border-radius: var(--border-radius);
            padding: 25px;
            text-align: center;
            box-shadow: var(--shadow-light);
            backdrop-filter: blur(20px);
            border: 1px solid rgba(255, 255, 255, 0.2);
            transition: var(--transition);
        }

        .tip-card:hover {
            transform: translateY(-5px);
            box-shadow: var(--shadow-medium);
        }

        .tip-icon {
            font-size: 2.5rem;
            margin-bottom: 15px;
        }

        .tip-card h3 {
            font-family: 'Playfair Display', serif;
            font-size: 1.4rem;
            color: var(--text-primary);
            margin-bottom: 15px;
        }

        .tip-card p {
            color: var(--text-secondary);
            line-height: 1.6;
            font-size: 0.95rem;
        }

        /* Booking Tips */
        .booking-tips {
            display: grid;
            gap: 30px;
        }

        .booking-tip h4 {
            color: var(--text-primary);
            font-size: 1.2rem;
            margin-bottom: 10px;
            display: flex;
            align-items: center;
            gap: 10px;
        }

        .booking-tip h4 i {
            color: #ff9a9e;
        }

        .booking-tip p {
            margin: 0;
        }

        /* Cuisine Grid */
        .cuisine-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
            gap: 25px;
            margin-top: 40px;
        }

        .food-card {
            background: var(--card-bg);
            border-radius: var(--border-radius);
            padding: 25px;
            text-align: center;
            box-shadow: var(--shadow-light);
            backdrop-filter: blur(20px);
            border: 1px solid rgba(255, 255, 255, 0.2);
            transition: var(--transition);
            position: relative;
            overflow: hidden;
        }

        .food-card:hover {
            transform: translateY(-5px);
            box-shadow: var(--shadow-medium);
        }

        .food-card::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 4px;
            background: var(--paris-gradient);
        }

        .food-icon {
            font-size: 2.5rem;
            margin-bottom: 15px;
        }

        .food-card h3 {
            font-family: 'Playfair Display', serif;
            font-size: 1.4rem;
            color: var(--text-primary);
            margin-bottom: 15px;
        }

        .food-card p {
            color: var(--text-secondary);
            line-height: 1.6;
            font-size: 0.95rem;
            margin-bottom: 15px;
        }

        .food-price {
            display: inline-block;
            background: var(--paris-gradient);
            color: var(--text-primary);
            padding: 5px 12px;
            border-radius: 15px;
            font-size: 0.9rem;
            font-weight: 600;
        }

        /* Seasons Grid */
        .seasons-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 25px;
            margin-top: 40px;
        }

        .season-card {
            background: var(--card-bg);
            border-radius: var(--border-radius);
            padding: 0;
            overflow: hidden;
            box-shadow: var(--shadow-light);
            backdrop-filter: blur(20px);
            border: 1px solid rgba(255, 255, 255, 0.2);
            transition: var(--transition);
        }

        .season-card:hover {
            transform: translateY(-5px);
            box-shadow: var(--shadow-medium);
        }

        .season-header {
            padding: 20px;
            text-align: center;
            background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%);
        }

        .season-card.spring .season-header {
            background: linear-gradient(135deg, #ffeaa7 0%, #fab1a0 100%);
        }

        .season-card.summer .season-header {
            background: linear-gradient(135deg, #fdcb6e 0%, #e17055 100%);
        }

        .season-card.autumn .season-header {
            background: linear-gradient(135deg, #e17055 0%, #d63031 100%);
        }

        .season-card.winter .season-header {
            background: linear-gradient(135deg, #74b9ff 0%, #0984e3 100%);
        }

        .season-icon {
            font-size: 2.5rem;
            margin-bottom: 10px;
        }

        .season-header h3 {
            font-family: 'Playfair Display', serif;
            font-size: 1.3rem;
            color: var(--text-primary);
            margin: 0;
        }

        .season-content {
            padding: 20px;
        }

        .season-content p {
            margin-bottom: 10px;
            font-size: 0.9rem;
            line-height: 1.5;
        }

        .season-rating {
            text-align: center;
            font-size: 1.2rem;
            color: #ffd700;
            margin-top: 15px;
        }

        /* Transport Grid */
        .transport-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
            gap: 25px;
            margin-top: 40px;
        }

        .transport-card {
            background: var(--card-bg);
            border-radius: var(--border-radius);
            padding: 25px;
            text-align: center;
            box-shadow: var(--shadow-light);
            backdrop-filter: blur(20px);
            border: 1px solid rgba(255, 255, 255, 0.2);
            transition: var(--transition);
            position: relative;
        }

        .transport-card:hover {
            transform: translateY(-5px);
            box-shadow: var(--shadow-medium);
        }

        .transport-icon {
            font-size: 2.5rem;
            margin-bottom: 15px;
        }

        .transport-card h3 {
            font-family: 'Playfair Display', serif;
            font-size: 1.4rem;
            color: var(--text-primary);
            margin-bottom: 15px;
        }

        .transport-card p {
            color: var(--text-secondary);
            line-height: 1.6;
            font-size: 0.95rem;
            margin-bottom: 20px;
            text-align: left;
        }

        .transport-price {
            background: var(--accent-gradient);
            color: var(--text-light);
            padding: 8px 16px;
            border-radius: 20px;
            font-size: 0.9rem;
            font-weight: 500;
            display: inline-block;
        }

        /* Responsive Design */
        @media (max-width: 768px) {
            .hero-left,
            .hero-right {
                transform: skewX(0);
                margin: 0;
            }

            .collage-image {
                transform: skewX(0) scale(1.2);
            }

            @keyframes kenBurns {
                0% { opacity: 0; transform: skewX(0) scale(1.2); }
                5% { opacity: 1; }
                20% { opacity: 1; transform: skewX(0) scale(1.3); }
                25% { opacity: 0; }
                100% { opacity: 0; transform: skewX(0) scale(1.2); }
            }
            
            .hero-content {
                padding: 2rem 1.5rem;
                max-width: 90%;
            }
            }

            .collage-image:nth-child(4) {
                grid-column: 1 / 4;
                grid-row: 3 / 4;
            }

            .collage-image:nth-child(5) {
                grid-column: 1 / 4;
                grid-row: 4 / 5;
            }
            
            .description-card {
                padding: 25px;
            }
            
            .image-gallery {
                grid-template-columns: 1fr;
                gap: 20px;
            }
            
            .stats-grid {
                grid-template-columns: repeat(2, 1fr);
                gap: 15px;
            }
            
            .quote-section {
                padding: 40px 25px;
            }
        }

        @media (max-width: 480px) {
            .nav-container {
                padding: 0 15px;
            }

            .hero-collage {
                grid-template-columns: repeat(2, 1fr);
                grid-template-rows: repeat(6, 1fr);
            }

            .collage-image:nth-child(1) {
                grid-column: 1 / 3;
                grid-row: 1 / 3;
            }

